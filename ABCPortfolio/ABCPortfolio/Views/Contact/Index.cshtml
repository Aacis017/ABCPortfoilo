@model ICollection<ContactUsViewModel>

    @{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
    }

<a href="~/lib/jquery/dist/jquery.min.map">~/lib/jquery/dist/jquery.min.map</a>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>
<a href="~/lib/bootstrap/dist/css/bootstrap.min.css.map">~/lib/bootstrap/dist/css/bootstrap.min.css.map</a>
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<a href="~/lib/bootstrap/dist/js/bootstrap.min.js.map">~/lib/bootstrap/dist/js/bootstrap.min.js.map</a>
<script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="~/js/jquery.min.js"></script>
<script src="~/css/jqueryui/jquery-ui.js"></script>
<script src="~/css/jqueryui/jquery-ui.min.js"></script>
<link href="~/lib/datatables/css/jquery.datatables.css" rel="stylesheet" />
<link href="~/lib/datatables/css/jquery.datatables.min.css" rel="stylesheet" />
<script src="~/lib/datatables/js/jquery.datatables.js"></script>
<script src="~/lib/datatables/js/jquery.datatables.min.js"></script>
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">User messages</h6>
    </div>
<div class="card-body">
    <div class="table-responsive">
           <table class="table table-bordered" id="contactUsdataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Service Type</th>
                        <th>Phone Number</th>
                        <th>Message</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Name</td>
                            <td>@item.Email</td>
                            <td>@item.ServiceType</td>
                            <td>@item.PhoneNo</td>
                            <td>@item.Message</td>
                        </tr>
                    }
                </tbody>
                <tfoot>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Service Type</th>
                        <th>Phone Number</th>
                        <th>Message</th>
                    </tr>
                </tfoot>
            </table>

            
    </div>
</div>
</div>
@section Scripts {
    <script src="~/js/datatable_file.js"></script>
}


